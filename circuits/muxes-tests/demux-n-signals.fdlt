(load "circuits/muxes.fdl")
(green input i)
(green output o)
(green input control)
(part-under-test (demux-6-signals i o control sig:signal-c sig:signal-i
                                  sig:signal-a sig:signal-b sig:signal-c
                                  sig:signal-d sig:signal-e sig:signal-f))
(test-type fixed-latency 2)

(test (((i sig:signal-i)
        (control sig:signal-c))
       ((o sig:signal-a)
        (o sig:signal-b)
        (o sig:signal-c)
        (o sig:signal-d)
        (o sig:signal-e)
        (o sig:signal-f)))
  ((1 0) (1 0 0 0 0 0))
  ((2 0) (2 0 0 0 0 0))
  ((2 1) (0 2 0 0 0 0))
  ((2 2) (0 0 2 0 0 0))
  ((2 3) (0 0 0 2 0 0))
  ((2 4) (0 0 0 0 2 0))
  ((2 5) (0 0 0 0 0 2)))
